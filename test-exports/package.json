{
  "name": "test-exports",
  "version": "0.0.0",
  "private": true,
  "description": "Tests for the things exported by the langchain package",
  "main": "./index.js",
  "type": "module",
  "scripts": {
    "test": "node ./index.mjs && tsc",
    "test:cf": "wrangler dev index.ts",
    "test:edge": "vercel dev ./vercel",
    "format": "prettier --write \"**/*.ts\"",
    "format:diff": "prettier --list-different \"**/*.ts\"",
    "ci": "yarn format:diff && yarn test"
  },
  "author": "Langchain",
  "license": "MIT",
  "dependencies": {
    "langchain": "workspace:*"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "prettier": "^2.8.3",
    "typescript": "^4.9.5",
    "vercel": "^28.16.6",
    "wrangler": "^2.11.0"
  }
}
